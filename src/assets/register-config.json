{
    "formTitle": "Registration",
    "saveBtnTitle": "Register",
    "resetBtnTitle": "Reset",
    "redirectLink":"/login",
    "redirectLinkTitle":"Please login here",
    "formControls": [
        {
            "type": "text",
            "name": "firstname",
            "label": "First Name",
            "placeholder": "Enter First Name",
            "class": "col-md-6",
            "value": "",
            "validators": [
                {
                    "validatorName": "required",
                    "required": true,
                    "message": "First Name is required"
                },
                {
                    "validatorName": "pattern",
                    "pattern": "^[a-zA-Z]+$",
                    "message": "Only Characters are allowed"
                }
            ]
        },
        {
            "type": "text",
            "name": "lastname",
            "label": "Last Name",
            "placeholder": "Enter Last Name",
            "class": "col-md-6",
            "value": "",
            "validators": [
                {
                    "validatorName": "required",
                    "required": true,
                    "message": "Last Name is required"
                },
                {
                    "validatorName": "pattern",
                    "pattern": "^[a-zA-Z]+$",
                    "message": "Only characters are allowed"
                }
            ]
        },
        {
            "type": "date",
            "name": "dateofbirth",
            "label": "Date Of Birth",
            "placeholder": "Date Of Birth",
            "class": "col-md-6",
            "value": "",
            "validators": [
                {
                    "validatorName": "required",
                    "required": true,
                    "message": "Date of birth is required"
                }
            ]
        },
        {
            "type": "select",
            "name": "technology",
            "value": "",
            "label": "Technology",
            "placeholder": "Select Technology",
            "class": "col-md-6",
            "options": [
                {
                    "id": "1",
                    "value": "Angular"
                },
                {
                    "id": "2",
                    "value": "Reactjs"
                },
                {
                    "id": "3",
                    "value": "Nodejs"
                }
            ],
            "validators": [
                {
                    "validatorName": "required",
                    "required": true,
                    "message": "Technology is required"
                }
            ]
        },
        {
            "type": "email",
            "name": "email",
            "placeholder": "Enter Email",
            "value": "",
            "class": "col-md-6",
            "label": "Email",
            "validators": [
                {
                    "validatorName": "required",
                    "message": "email is required",
                    "required": true
                },
                {
                    "validatorName": "email",
                    "message": "Enter valid email",
                    "email": "email"
                }
            ]
        },
        {
            "type": "password",
            "name": "password",
            "placeholder": "Enter Password",
            "value": "",
            "class": "col-md-6",
            "label": "Password",
            "validators": [
                {
                    "validatorName": "required",
                    "message": "password is required",
                    "required": true
                },
                {
                    "validatorName": "pattern",
                    "pattern": "^(?=.*[a-z])(?=.*[A-Z])(?=.*\\d)(?=.*[@$!%*?&])[A-Za-z\\d@$!%*?&]{8,}$",
                    "message": "At least 8 characters long, one uppercase letter, lowercase letter , digit & special character (such as !@#$%^&*)."
                }
            ]
        },
        {
            "type": "radio",
            "name": "gender",
            "value": "",
            "label": "Gender",
            "placeholder": "Select Gender",
            "class": "col-md-6",
            "radioOptions": [
                "Male",
                "Female"
            ],
            "validators": [
                {
                    "validatorName": "required",
                    "required": true,
                    "message": "Gender is required"
                }
            ]
        }
    ]
